    "   \"execution_count\": 12,\n",
    "   \"metadata\": {\n",
    "    \"ExecuteTime\": {\n",
    "     \"end_time\": \"2025-07-31T10:25:08.756495Z\",\n",
    "     \"start_time\": \"2025-07-31T10:25:02.518692Z\"\n",
    "    }\n",
    "   },\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"name\": \"stdout\",\n",
    "     \"output_type\": \"stream\",\n",
    "     \"text\": [\n",
    "      \"[Parallel(n_jobs=4)]: Using backend LokyBackend with 4 concurrent workers.\\n\",\n",
